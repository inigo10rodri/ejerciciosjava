import processing.core.PApplet;

public class PingPong extends PApplet {
	//Variables
	int x;
	int y;
	public void setup(){
		size(450,450);
		x=100;
		y=100;
	}
	public void draw(){
		background(0,0,0);
		ellipse(x,y,45,45);
		x=x+3;
		
	}
	
public static void main(String[] args) {
		
		PApplet.main("PingPong");
}
}
