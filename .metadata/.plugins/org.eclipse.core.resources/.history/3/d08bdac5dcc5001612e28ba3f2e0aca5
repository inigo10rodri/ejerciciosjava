import java.util.Scanner;

/*Realizar un programa que pida al usuario 10 números y los muestra por consola en 
el orden inverso al introducido.*/
public class Ejercicio3 {

	public static void main(String[] args) {
		//Iniciar scanner
		Scanner sc=new Scanner(System.in);
		//Variables
		final int MAX_LENGHT=10;
		int Numeros[]=new int [MAX_LENGHT];
		
		for(int i=MAX_LENGHT-1;i<=0;i--){
			System.out.println("Introduce una nota: ");
			Numeros[i]=sc.nextInt();
		}
		for(int i=0;i<=MAX_LENGHT;i++){
			System.out.print(Numeros[i]+" ");
		}
		/*Scanner tc=new Scanner(System.in);
		final int MAX_LENGTH=10;
		int num[]=new int[MAX_LENGTH];
		
		for(int i=MAX_LENGTH-1;i>=0;i--){
			System.out.println("Ingrese un número : ");
			num[i]=tc.nextInt();*/	
	}
}

