import java.util.Scanner;
public class Ejercicio3 {

	public static void main(String[] args) {
		//Iniciar scanner
		Scanner sc=new Scanner(System.in);
		//variables
		double nota = 0;
		double menor = 0;
		double mayor=0;
		int cont=0;
		double suma=0;
		double notaMedia=0;
		for(int i=1;nota<=10;i++){
			//Introduce una nota
			System.out.print("Introducir nota "+i+":");
			//Leer nota
			nota=sc.nextDouble();
			if(nota>10){
				System.out.println("Introduce un numero entre 0 y 10");
			}//if
			else if (nota>=0 || nota<=10){
				suma=nota+suma;
				cont++;
				notaMedia=suma/cont;
			}else if(nota<nota){
				nota=menor;				
			}			
			else if(notaMedia>=5 && notaMedia<6){
				System.out.print("Tienes un SUFICIENTE ("+notaMedia+")");
			}
			else if(notaMedia>=6 && notaMedia<7){
				System.out.print("Tienes un BIEN ("+notaMedia+")");
			}
			else if(notaMedia>=7 && notaMedia<=8){
				System.out.print("Tienes un NOTABLE ("+notaMedia+")");
			}
			else if(notaMedia>=9 && notaMedia<=10){
				System.out.print("Tienes un SOBRESALIENTE ("+notaMedia+")");
			}
			else if(notaMedia<5){
				System.out.print("Tienes un INSUFICIENTE ("+notaMedia+")");
			}
			else{
				System.out.print("ERROR!");
			}
		}		
		System.out.print("Mejor nota: "+mayor);
		System.out.print("Peor nota: "+menor);
		System.out.print("Nota media: "+(suma/cont));
	}//main
}//class